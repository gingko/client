{
  "name": "gingko-client",
  "productName": "Gingko",
  "version": "0.1.3",
  "main": "electron-start.js",
  "description": "Gingko client rewritten in Elm, packaged with Electron.",
  "scripts": {
    "build-web": "rm -rf ./dist/ && mkdir -p ./dist/static && cp -r ./src/static/* ./dist/static && TARGET=web webpack --watch",
    "build-electron": "rm -rf ./dist/ && mkdir -p ./dist/static && cp -r ./src/static/* ./dist/static && TARGET=electron webpack --watch",
    "elm-web": "elm-make src/elm/Main.elm --output src/elm.js && TARGET=web webpack --watch",
    "elm-electron": "elm-make src/elm/Main.elm --output src/elm.js && TARGET=electron webpack --watch",
    "postinstall": "electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AdrianoFerrari/gingko-client.git"
  },
  "author": "Adriano Ferrari",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AdrianoFerrari/gingko-client/issues"
  },
  "homepage": "https://github.com/AdrianoFerrari/gingko-client",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "electron": "^1.4.14",
    "electron-rebuild": "^1.5.7",
    "elm-webpack-loader": "^4.3.0",
    "imports-loader": "^0.7.1",
    "webpack": "^2.3.0"
  },
  "dependencies": {
    "electron-machine-id": "^1.0.9",
    "jquery": "^3.1.1",
    "mz": "^2.6.0",
    "pouchdb": "^6.1.2",
    "sha1": "^1.1.1",
    "textarea-autosize": "^0.4.2",
    "underscore": "^1.8.3"
  }
}
